<?xml version="1.0" encoding="UTF-8"?>
<SolverDefinition 
	xmlns="www.iric.net/SolverDefinition/1.0"
	name="Mflow_02"
	caption="Mflow_02 iRIC4 ver1.1"
	version="3.2022.1"
	copyright="Mineyuki Gamou"
	release="2022.07.01"
	homepage="http://i-ric.org"
	executable="Mflow22_02_ver2_iric4.exe"
	iterationtype="time"
	gridtype="unstructured2d"
>
	<CalculationCondition>
		<Tab name="Case" caption="Case">
			<Content>
				<Items>
					<Item name="TITLE" caption="Case">
						<Definition conditionType="constant" valueType="string" />
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="Control specification" caption="Control specification">
			<Content>
				<Items>
					<Item name="START_TIME" caption="Start time (s)">
						<Definition conditionType="constant" valueType="real" option="false" default="0.0"/>
					</Item>
					<Item name="DLETA_T" caption="Computation time step (s)">
						<Definition conditionType="constant" valueType="real" option="false" default="0.1"/>
					</Item>
					<Item name="END_TIME" caption="End time (s)">
						<Definition conditionType="constant" valueType="real" option="false" default="0.0"/>
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="Turbulent model" caption="Turbulent model">
			<Content>
				<Items>
					<Item name="ICON_09" caption="Turbulent model">
						<Definition conditionType="constant" valueType="integer" option="true" default="2">
							<Enumerations>
								<Enumeration value="1" caption="K-epsilon (simple) model"/>
								<Enumeration value="2" caption="Zero equation model"/>
								<Enumeration value="3" caption="Specific value of eddy viscosity coefficient"/>
							</Enumerations>
						</Definition>
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="Initial condition" caption="Initial condition">
			<Content>
				<Items>
					<Item name="IBH_00" caption="Data source">
						<Definition conditionType="constant" valueType="integer" option="true" default="1">
							<Enumerations>
								<Enumeration value="0" caption="Without using Area cord"/>
								<Enumeration value="1" caption="Using Area cord"/>
							</Enumerations>
						</Definition>
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="File control" caption="File control">
			<Content>
				<Items>
					<Item name="ICON_03" caption="Computation from initial or restart file">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="Initial"/>
								<Enumeration value="1" caption="Restart"/>
							</Enumerations>
						</Definition>
					</Item>
					<GroupBox caption="Restart file">
						<Item name="IR_FILE" caption="     Input restart file name">
							<Definition conditionType="constant" valueType="filename" option="false" default="Select File">
								<Dependency>
									<Condition type="isEqual" target="ICON_03" value="1"/>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
						<Item name="ICON_04" caption="     Output of last step to restart file">
							<Definition conditionType="constant" valueType="integer" option="true" default="0">
								<Enumerations>
									<Enumeration value="0" caption="No"/>
									<Enumeration value="1" caption="Yes"/>
								</Enumerations>
							</Definition>
						</Item>
						<Item name="OR_FILE" caption="     Output restart file name">
							<Definition conditionType="constant" valueType="string" option="false" default="Full pass file name">
								<Dependency>
									<Condition type="isEqual" target="ICON_04" value="1"/>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
 					</GroupBox>
					<GroupBox caption="iRIC file">
						<Item name="IRIC_STR_TIME" caption="     Start time of output to iRIC (s)">
							<Definition conditionType="constant" valueType="real" option="false" default="10"/>
						</Item>
						<Item name="IRIC_DLT_TIME" caption="     Time interval to iRIC (s)">
							<Definition conditionType="constant" valueType="real" option="false" default="10"/>
						</Item>
 					</GroupBox>
					<GroupBox caption="External text file">
						<Item name="ICON_01" caption="     Output to external file">
							<Definition conditionType="constant" valueType="integer" option="true" default="1">
								<Enumerations>
									<Enumeration value="0" caption="Enabled"/>
									<Enumeration value="1" caption="Disabled"/>
								</Enumerations>
							</Definition>
						</Item>
						<Item name="PRINT_FILE" caption="     Name of external text file">
							<Definition conditionType="constant" valueType="string" option="false" default="Full pass file name">
								<Dependency>
									<Condition type="isEqual" target="ICON_01" value="0"/>
									<Action type="enable"/>
								</Dependency>
							</Definition>     
						</Item>
						<Item name="PS_TIME" caption="     Start time of output to external text file (s)">
							<Definition conditionType="constant" valueType="real" option="false" default="10">
								<Dependency>
									<Condition type="isEqual" target="ICON_01" value="0"/>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
						<Item name="PDL_TIME" caption="     Time interval to external text file (s)">
							<Definition conditionType="constant" valueType="real" option="false" default="10">
								<Dependency>
									<Condition type="isEqual" target="ICON_01" value="0"/>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
 					</GroupBox>
					<GroupBox caption="External dump file (text file)">
						<Item name="ICON_05" caption="     Output to external dump file">
							<Definition conditionType="constant" valueType="integer" option="true" default="0">
								<Enumerations>
									<Enumeration value="0" caption="Disabled"/>
									<Enumeration value="1" caption="Enabled"/>
								</Enumerations>
							</Definition>
						</Item>
						<Item name="PLOT_FILE" caption="     Name of external dump file">
							<Definition conditionType="constant" valueType="string" option="false" default="Full pass file name">
								<Dependency>
									<Condition type="isEqual" target="ICON_05" value="1"/>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
						<Item name="FS_TIME" caption="     Start time of output to dump file (s)">
							<Definition conditionType="constant" valueType="real" option="false" default="10">
								<Dependency>
									<Condition type="isEqual" target="ICON_05" value="1"/>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
						<Item name="DL_TIME" caption="     Time interval to dump file (s)">
							<Definition conditionType="constant" valueType="real" option="false" default="10">
								<Dependency>
									<Condition type="isEqual" target="ICON_05" value="1"/>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
 					</GroupBox>
				</Items>
			</Content>
		</Tab>
		<Tab name="vegetation / wind" caption="Vegetation / Wind">
			<Content>
				<Items>
					<Item name="ICON_15" caption="Vegetation">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="Disabled" />
								<Enumeration value="1" caption="Enabled"/>
							</Enumerations>
						</Definition>
					</Item>
					<Item name="ICON_14" caption="Wind stress">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="Disabled"/>
								<Enumeration value="1" caption="Enabled"/>
							</Enumerations>
						</Definition>
					</Item>
					<Item name="WIND_MAG" caption="     Wind magnitude at 10m above ground">
						<Definition conditionType="constant" valueType="real" option="false" default="0.0">
							<Dependency>
								<Condition type="isEqual" target="ICON_14" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="WIND_DIR" caption="     Wind direction">
						<Definition conditionType="constant" valueType="real" option="false" default="0.0">
							<Dependency>
								<Condition type="isEqual" target="ICON_14" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="Movable bed computation" caption="Movable bed computation">
			<Content>
				<Items>
					<Item name="ICON_13" caption="Movable bed computation">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="Flow only" />
								<Enumeration value="1" caption="Movable bed computation" />
							</Enumerations>
						</Definition>
					</Item>
					<Item name="BK_START" caption="Movable bed computation start time (s)">
						<Definition conditionType="constant" valueType="real" option="false" default="1.0">
							<Dependency>
						  		<Condition type="isEqual" target="ICON_13" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="BK_END" caption="Movable bed computation end time (s)">
						<Definition conditionType="constant" valueType="real" option="false" default="1.0">
							<Dependency>
						  		<Condition type="isEqual" target="ICON_13" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="BK_EXTK" caption="Morphological factor">
						<Definition conditionType="constant" valueType="real" option="false" default="1.0">
							<Dependency>
						  		<Condition type="isEqual" target="ICON_13" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<GroupBox caption="Bedload transport">
						<Item name="IFCS_00" caption="     Bedload transport equation">
							<Definition conditionType="constant" valueType="integer" option="true" default="0">
								<Enumerations>
									<Enumeration value="0" caption="M.P.M formula"/>
									<Enumeration value="1" caption="Ashida and Michiue formula"/>
									<Enumeration value="2" caption="Engelund Hansen formula"/>
								</Enumerations>
								<Dependency>
								        <Condition type="isEqual" target="ICON_13" value="1"/>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
						<Item name="IBF_00" caption="     Limit of scouring">
							<Definition conditionType="constant" valueType="integer" option="true" default="0">
								<Enumerations>
									<Enumeration value="0" caption="No limit" />
									<Enumeration value="1" caption="Yes limit" />
								</Enumerations>
								<Dependency>
									<Condition type="isEqual" target="ICON_13" value="1"/>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
						<Item name="SNS" caption="     Secondary flow coefficient">
							<Definition conditionType="constant" valueType="real" option="false" default="7">
								<Dependency>
									<Condition type="isEqual" target="ICON_13" value="1"/>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
						<Item name="S_S" caption="     Relative weight of bed material">
							<Definition conditionType="constant" valueType="real" option="false" default="1.65">
								<Dependency>
									<Condition type="isEqual" target="ICON_13" value="1"/>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
						<Item name="RAMDA" caption="     Void ratio of bed material (%)">
							<Definition conditionType="constant" valueType="real" option="false" default="30">
								<Dependency>
									<Condition type="isEqual" target="ICON_13" value="1"/>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
					</GroupBox>
					<GroupBox caption="Suspended load">
						<Item name="ICON_12" caption="     Suspended load">
							<Definition conditionType="constant" valueType="integer" option="true" default="0">
								<Enumerations>
									<Enumeration value="0" caption="Bed load only" />
									<Enumeration value="1" caption="Bed and suspended load" />
								</Enumerations>
								<Dependency>
								        <Condition type="isEqual" target="ICON_13" value="1"/>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
						<Item name="IEES_00" caption="     Non-dimensional velocity of buoyancy">
							<Definition conditionType="constant" valueType="integer" option="true" default="0">
								<Enumerations>
									<Enumeration value="0" caption="Kishi-Itakura formula" />
									<Enumeration value="1" caption="Garcia-Paker formula" />
									<Enumeration value="2" caption="Einstein formula" />
								</Enumerations>
								<Dependency>
									<Condition type="and">
							  			<Condition type="isEqual" target="ICON_12" value="1"/>
							  			<Condition type="isEqual" target="ICON_13" value="1"/>
									</Condition>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
						<Item name="ICAS_00" caption="     Reference concentration">
							<Definition conditionType="constant" valueType="integer" option="true" default="0">
								<Enumerations>
									<Enumeration value="0" caption="Lane-Kalinske development formula" />
									<Enumeration value="1" caption="Lane-Kalinske formula" />
									<Enumeration value="2" caption="Rause formula" />
								</Enumerations>
								<Dependency>
									<Condition type="and">
							  			<Condition type="isEqual" target="ICON_12" value="1"/>
							  			<Condition type="isEqual" target="ICON_13" value="1"/>
									</Condition>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
						<Item name="ITYPE_SUS" caption="     Type of diffusion coefficient of suspended load">
							<Definition conditionType="constant" valueType="integer" option="true" default="2">
								<Enumerations>
									<Enumeration value="1" caption="Specific value"/>
									<Enumeration value="2" caption="Equal to eddy viscosity coefficient"/>
								</Enumerations>
								<Dependency>
									<Condition type="and">
								  		<Condition type="isEqual" target="ICON_12" value="1"/>
								  		<Condition type="isEqual" target="ICON_13" value="1"/>
									</Condition>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
					</GroupBox>
					<GroupBox caption="Grain size distribution">
						<Item name="IDP_00" caption="     Bed material dictribution">
							<Definition conditionType="constant" valueType="integer" option="true" default="0">
								<Enumerations>
									<Enumeration value="0" caption="Uniform" />
									<Enumeration value="1" caption="Non-uniform" />
								</Enumerations>
								<Dependency>
							  		<Condition type="isEqual" target="ICON_13" value="1"/>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
						<Item name="PCT_00" caption="     Grain diameter (mm)">
							<Definition conditionType="constant" valueType="real" option="false" default="10">
								<Dependency>
									<Condition type="and">
										<Condition type="isEqual" target="ICON_13" value="1"/>
								  		<Condition type="isEqual" target="IDP_00" value="0"/>
									</Condition>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
						<Item name="I_STRESS"  caption="     Friction velocity">
							<Definition conditionType="constant" valueType="integer" option="true" default="0">
								<Enumerations>
									<Enumeration value="0" caption="Calculated from water-surface gradient" />
									<Enumeration value="1" caption="Calculated from equivalent roughness height" />
								</Enumerations>
								<Dependency>
									<Condition type="and">
										<Condition type="isEqual" target="ICON_13" value="1"/>
										<Condition type="isEqual" target="IDP_00" value="1" />
									</Condition>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
						<Item name="DDM" caption="     Exchange layer thickness (m)">
							<Definition conditionType="constant" valueType="real" option="false" default="0.50">
								<Dependency>
									<Condition type="and">
										<Condition type="isEqual" target="ICON_13" value="1"/>
										<Condition type="isEqual" target="IDP_00" value="1" />
									</Condition>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
						<Item name="ISDM" caption="     Grain size distribution ob transrate and substrate layer">
							<Definition conditionType="constant" valueType="integer" option="true" default="0">
								<Enumerations>
									<Enumeration value="0" caption="Same as exchange layer" />
									<Enumeration value="1" caption="Different from exchange layer" />
								</Enumerations>
								<Dependency>
									<Condition type="and">
										<Condition type="isEqual" target="ICON_13" value="1"/>
										<Condition type="isEqual" target="IDP_00" value="1" />
									</Condition>
									<Action type="enable"/>
								</Dependency>
							</Definition>
						</Item>
					</GroupBox>
				</Items>
			</Content>
		</Tab>
		<Tab name="Internal in/out flow" caption="Internal in/out flow">
			<Content>
				<Items>
					<Item name="ICON_10" caption="In/out flow on closed boundary">
						<Definition conditionType="constant" valueType="integer" option="true" default="0" >
							<Enumerations>
								<Enumeration value="0" caption="Disabled"/>
								<Enumeration value="1" caption="Enabled"/>
							</Enumerations>
						</Definition>
					</Item>
					<Item name="ICON_11" caption="In/out flow on bottom">
						<Definition conditionType="constant" valueType="integer" option="true" default="0" >
							<Enumerations>
								<Enumeration value="0" caption="Disabled"/>
								<Enumeration value="1" caption="Enabled"/>
							</Enumerations>
						</Definition>
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="Dike break" caption="Dike break">
			<Content>
				<Items>
					<Item name="ICON_16" caption="Dike break">
						<Definition conditionType="constant" valueType="integer" option="true" default="0" >
							<Enumerations>
								<Enumeration value="0" caption="Disabled"/>
								<Enumeration value="1" caption="Enabled"/>
							</Enumerations>
						</Definition>
					</Item>
				</Items>
			</Content>
		</Tab>
	</CalculationCondition>
	<GridRelatedCondition>
		<Item name="Elevation" caption="Elevation (m)">
			<Definition position="node" valueType="real" default="max" keepOrder="true"/>
		</Item>
		<Item name="AREA_CORD" caption="Area code">
			<Definition valueType="complex" keepOrder="true">
				<Item name="IAREA" caption="Data source of initial condition">
					<Definition conditionType="constant" valueType="integer" option="true" default="0" keepOrder="true">
						<Enumerations>
							<Enumeration value="0" caption="No setting" />
							<Enumeration value="1" caption="Setting up water depth using area cord"/>
							<Enumeration value="2" caption="Setting up water depth and grand elevation using area cord"/>
							<Enumeration value="3" caption="Setting up water surface elevation (flat)"/>
						</Enumerations>
					</Definition>
				</Item>
				<GroupBox caption="River survey file">
					<Item name="SURVEY_FILE" caption="     River survey file name">
						<Definition conditionType="constant" valueType="filename" option="false" default="Select File" keepOrder="true">
							<Condition type="or">
								<Condition type="isEqual" target="IAREA" value="1"/>
								<Condition type="isEqual" target="IAREA" value="2"/>
							</Condition>
							<Action type="enable" />
						</Definition>
					</Item>
					<Item name="HQ_00" caption="     Initial water-elevation at downstream">
						<Definition conditionType="constant" valueType="real" option="true" default="0" keepOrder="true">
							<Condition type="or">
								<Condition type="isEqual" target="IAREA" value="1"/>
								<Condition type="isEqual" target="IAREA" value="2"/>
							</Condition>
							<Action type="enable" />
						</Definition>
					</Item>
					<Item name="DSX_00" caption="     Initial water-surface slope">
						<Definition conditionType="constant" valueType="real" option="true" default="0" keepOrder="true">
							<Condition type="or">
								<Condition type="isEqual" target="IAREA" value="1"/>
								<Condition type="isEqual" target="IAREA" value="2"/>
							</Condition>
							<Action type="enable" />
						</Definition>
					</Item>
				</GroupBox>
				<GroupBox caption="Water surface elevation (flat)">
					<Item name="WATER_S" caption="     Water surface elevation">
						<Definition conditionType="constant" valueType="real" option="true" default="0" keepOrder="true">
							<Condition type="isEqual" target="IAREA" value="3"/>
							<Action type="enable" />
						</Definition>
					</Item>
				</GroupBox>
			</Definition>
		</Item>
		<Item name="DEPTH_00" caption="Initial depth (m)">
			<Definition position="node" valueType="real" default="0" keepOrder="true"/>
		</Item>
		<Item name="ROUGH_MN" caption="Manning roughness">
			<Definition position="cell" valueType="real" default="0.03" keepOrder="true"/>
		</Item>
		<Item name="EDY_CO_PAA" caption="Eddy viscosity coefficient(paramter A)">
			<Definition position="cell" valueType="real" default="1" keepOrder="true"/>
		</Item>
		<Item name="EDY_CO_DIN" caption="Eddy viscosity coefficient(Specific value)">
			<Definition position="cell" valueType="real" default="1" keepOrder="true"/>
		</Item>
		<Item name="TR_EFCT" caption="Vegetation">
			<Definition position="cell" valueType="complex" keepOrder="true">
				<Item name="TR_COD" caption="Vegetation">
					<Definition conditionType="constant" valueType="integer" option="true" default="0" keepOrder="true">
						<Enumerations>
							<Enumeration value="0" caption="No setting" />
							<Enumeration value="1" caption="Vegetation density" />
							<Enumeration value="2" caption="Permeability coefficient" />
						</Enumerations>
					</Definition>
				</Item>
				<Item name="TR_COE1" caption="     Vegetation density">
					<Definition position="cell" valueType="real" default="0" keepOrder="true">
						<Condition type="isEqual" target="TR_COD" value="1"/>
						<Action type="enable" />
					</Definition>
				</Item>
				<Item name="TR_COE2" caption="     Permeability coefficient" >
					<Definition position="cell" valueType="real" option="true" default="0" keepOrder="true">
						<Condition type="isEqual" target="TR_COD" value="2"/>
						<Action type="enable" />
					</Definition>
				</Item>
				<Item name="VEG_HT" caption="     Vegetation height (m)" >
					<Definition position="cell" valueType="real" option="true" default="0" keepOrder="true">
						<Condition type="isEqual" target="TR_COD" value="2"/>
						<Action type="enable" />
					</Definition>
				</Item>
			</Definition>
		</Item>
		<Item name="IBF_COD" caption="Limit of scouring">
			<Definition position="node" valueType="complex" keepOrder="true">
				<Item name="IBF_TYP" caption="Type of limit of scouring" >
					<Definition position="node" conditionType="constant" valueType="integer" option="true" default="0" keepOrder="true">
						<Enumerations>
							<Enumeration value="0" caption="No limit" />
							<Enumeration value="1" caption="Scour depth control" />
							<Enumeration value="2" caption="Scour elevation control" />
							<Enumeration value="3" caption="Fixed bed" />
						</Enumerations>
					</Definition>
				</Item>
				<Item name="BLDPT" caption="     Scour depth control (m)">
					<Definition position="node" valueType="real" default="0" keepOrder="true">
						<Condition type="isEqual" target="IBF_TYP" value="1"/>
						<Action type="enable" />
					</Definition>
				</Item>
				<Item name="BLHIT" caption="     Scour elevation control (m)">
					<Definition position="node" valueType="real" default="0" keepOrder="true">
						<Condition type="isEqual" target="IBF_TYP" value="2"/>
						<Action type="enable" />
					</Definition>
				</Item>
			</Definition>
		</Item>
		<Item name="GRAIN_SIZE_EX" caption="Grain size distribution (exchange layer)">
			<Definition position="node" valueType="complex" keepOrder="true">
				<Item name="IG_PRT_EX" caption="Grain size distribution" >
					<Definition position="node" conditionType="constant" valueType="integer" option="true" default="0" keepOrder="true">
						<Enumerations>
							<Enumeration value="0" caption="No setting" />
							<Enumeration value="1" caption="Setting" />
						</Enumerations>
					</Definition>
				</Item>
				<Item name="IGS_CORD_EX" caption="Pattern of grain size distribution">
					<Definition position="node" conditionType="functional" keepOrder="true">
						<Parameter name="DPAI_01_EX" valueType="real" caption="Diameter(mm)" />
						<Value name="PCT_01_EX" valueType="real" caption="Fraction(%)" />
						<Condition type="isEqual" target="IG_PRT_EX" value="1"/>
						<Action type="enable" />
					</Definition>
				</Item>
			</Definition>
		</Item>
		<Item name="GRAIN_SIZE_TR" caption="Grain size distribution (transrate layer)">
			<Definition position="node" valueType="complex" keepOrder="true">
				<Item name="IG_PRT_TR" caption="Grain size distribution" >
					<Definition position="node" conditionType="constant" valueType="integer" option="true" default="0" keepOrder="true">
						<Enumerations>
							<Enumeration value="0" caption="No setting" />
							<Enumeration value="1" caption="Setting" />
						</Enumerations>
					</Definition>
				</Item>
				<Item name="IGS_CORD_TR" caption="Pattern of grain size distribution">
					<Definition position="node" conditionType="functional" keepOrder="true">
						<Parameter name="DPAI_01_TR" valueType="real" caption="Diameter(mm)" />
						<Value name="PCT_01_TR" valueType="real" caption="Fraction(%)" />
						<Condition type="isEqual" target="IG_PRT_TR" value="1"/>
						<Action type="enable" />
					</Definition>
				</Item>
			</Definition>
		</Item>
		<Item name="GRAIN_SIZE_SB" caption="Grain size distribution (substrate layer)">
			<Definition position="node" valueType="complex" keepOrder="true">
				<Item name="IG_PRT_SB" caption="Grain size distribution" >
					<Definition position="node" conditionType="constant" valueType="integer" option="true" default="0" keepOrder="true">
						<Enumerations>
							<Enumeration value="0" caption="No setting" />
							<Enumeration value="1" caption="Setting" />
						</Enumerations>
					</Definition>
				</Item>
				<Item name="IGS_CORD_SB" caption="Pattern of grain size distribution">
					<Definition position="node" conditionType="functional" keepOrder="true">
						<Parameter name="DPAI_01_SB" valueType="real" caption="Diameter(mm)" />
						<Value name="PCT_01_SB" valueType="real" caption="Fraction(%)" />
						<Condition type="isEqual" target="IG_PRT_SB" value="1"/>
						<Action type="enable" />
					</Definition>
				</Item>
			</Definition>
		</Item>
		<Item name="DIFF_CO" caption="Diffusion coefficient of suspended load(Specific value)">
			<Definition position="cell" valueType="real" default="1" keepOrder="true"/>
		</Item>
		<Item name="HTNZ_COD" caption="Dike break">
			<Definition position="node" valueType="complex" keepOrder="true">
				<Item name="IHT_TYP" caption="Condition of dike broken or sheathing board down" >
					<Definition position="node" conditionType="constant" valueType="integer" option="true" default="0" keepOrder="true">
						<Enumerations>
							<Enumeration value="0" caption="No setting" />
							<Enumeration value="1" caption="Setting up dike broken by time" />
							<Enumeration value="2" caption="Setting up dike broken by water elevation" />
							<Enumeration value="3" caption="Setting up sheathing board down" />
						</Enumerations>
					</Definition>
				</Item>
				<GroupBox caption="Condition of dike broken">
					<Item name="HTNZZZ" caption="     Elevation after dike broken (m)">
						<Definition position="node" valueType="real" default="0" keepOrder="true">
							<Condition type="or">
								<Condition type="isEqual" target="IHT_TYP" value="1"/>
								<Condition type="isEqual" target="IHT_TYP" value="2"/>
							</Condition>
							<Action type="enable" />
						</Definition>
					</Item>
					<Item name="HTTIME" caption="     Dike broken time (s)">
						<Definition position="node" valueType="real" default="0" keepOrder="true">
							<Condition type="isEqual" target="IHT_TYP" value="1"/>
							<Action type="enable" />
						</Definition>
					</Item>
					<Item name="HTLEVEL" caption="     Water elevation of dike broken (m)">
						<Definition position="node" valueType="real" default="0" keepOrder="true">
							<Condition type="isEqual" target="IHT_TYP" value="2"/>
							<Action type="enable" />
						</Definition>
					</Item>
				</GroupBox>
				<GroupBox caption="Condition of sheathing board down">
					<HBoxLayout>
						<Label row="0" col="0" caption="     Elevation after sheathing board down (m)"/>
						<Item row="0" col="1" name="SHTZZZ" >
							<Definition position="node" valueType="real" default="0" keepOrder="true">
								<Condition type="isEqual" target="IHT_TYP" value="3"/>
								<Action type="enable" />
							</Definition>
						</Item>
					</HBoxLayout>
					<HBoxLayout>
						<Label row="0" col="0" caption="     Sheathing board down stert time (s)"/>
						<Item row="1" col="0" name="HTSTART" >
							<Definition position="node" valueType="real" default="0" keepOrder="true">
								<Condition type="isEqual" target="IHT_TYP" value="3"/>
								<Action type="enable" />
							</Definition>
						</Item>
						<Label row="0" col="0" caption="     Sheathing board down end time (s)"/>
						<Item row="1" col="1" name="HTEND" >
							<Definition position="node" valueType="real" default="0" keepOrder="true">
								<Condition type="isEqual" target="IHT_TYP" value="3"/>
								<Action type="enable" />
							</Definition>
						</Item>
					</HBoxLayout>
				</GroupBox>
			</Definition>
		</Item>
		<Item name="IN_OUT_FLOW_CB" caption="In/out flow on closed boundary">
			<Definition position="node" valueType="complex" keepOrder="true">
				<Item name="ICB_PRT" caption="In/out flow on closed boundary" >
					<Definition position="node" conditionType="constant" valueType="integer" option="true" default="0" keepOrder="true">
						<Enumerations>
							<Enumeration value="0" caption="No setting" />
							<Enumeration value="1" caption="Setting" />
						</Enumerations>
					</Definition>
				</Item>
				<Item name="ETCBQQ_00" caption="In/out flow at node on closed boundary">
					<Definition position="node" conditionType="functional" keepOrder="true">
						<Parameter name="ETSUT_01" valueType="real" caption="Time (s)"/>
						<Value name="ETSUQ_01" valueType="real" caption="Flow (m3/s)"/>
						<Dependency>
							<Condition type="isEqual" target="ICB_PRT" value="1"/>
							<Action type="enable"/>
						</Dependency>
					</Definition>
				</Item>
			</Definition>
		</Item>
		<Item name="IN_OUT_FLOW_BTM" caption="In/out flow on bottom">
			<Definition position="cell" valueType="complex" keepOrder="true">
				<Item name="IBTM_PRT" caption="In/out flow on bottom" >
					<Definition position="cell" conditionType="constant" valueType="integer" option="true" default="0" keepOrder="true">
						<Enumerations>
							<Enumeration value="0" caption="No setting" />
							<Enumeration value="1" caption="Setting" />
						</Enumerations>
					</Definition>
				</Item>
				<Item name="ETBTMQQ_00" caption="In/out flow at element on bottom">
					<Definition position="cell" conditionType="functional" keepOrder="true">
						<Parameter name="BTMT_01" valueType="real" caption="Time (s)"/>
						<Value name="BTMQ_01" valueType="real" caption="Flow (m3/s)"/>
						<Dependency>
							<Condition type="isEqual" target="IBTM_PRT" value="1"/>
							<Action type="enable"/>
						</Dependency>
					</Definition>
				</Item>
			</Definition>
		</Item>
	</GridRelatedCondition>
	<BoundaryCondition name="IPON_PARM" caption="Boundary condition" position="node">
		<GroupBox caption="Type of boundary condition">
			<Item name="IBCON_13" caption="     Type of boundary condition">
				<Definition conditionType="constant" valueType="integer" option="true" default="0">
					<Enumerations>
						<Enumeration value="0" caption="Flow" />
						<Enumeration value="1" caption="Flow and movable bed computation" />
						<Enumeration value="2" caption="Flow , movable bed and suspended load" />
					</Enumerations>
				</Definition>
			</Item>
		</GroupBox>
		<HBoxLayout>
			<GroupBox caption="Boundary condition of flow">
				<Item name="IPON_IN" caption="     Type of boundary condition of flow">
					 <Definition valueType="integer" default="3" >
						<Enumeration value="1" caption="Tidal condition" />
						<Enumeration value="2" caption="Time varying elevation (outflow)" />
						<Enumeration value="3" caption="Time varying discharge" />
						<Enumeration value="4" caption="H - Q distributioin (restart only)" />
						<Enumeration value="5" caption="Time varying elevation and discharge" />
						<Enumeration value="6" caption="Time varying elevation (inflow)" />
					</Definition>
				</Item>
				<Item name="T_PERIOD" caption="          Tidal period (s)">
					<Definition position="node" valueType="real" default="43200.0" >
						<Condition type="isEqual" target="IPON_IN" value="1"/>
					</Definition>
				</Item>
				<Item name="AMPLITUDE" caption="          Amplitude (m)">
					<Definition position="node" valueType="real" default="1.0" option="false">
						<Condition type="isEqual" target="IPON_IN" value="1"/>
					</Definition>
				</Item>
				<Item name="PHASE_S" caption="          Phase of starting point (s)">
					<Definition position="node" valueType="real" default="0.0" option="false">
						<Condition type="isEqual" target="IPON_IN" value="1"/>
					</Definition>
				</Item>
				<Item name="PHASE_E" caption="          Phase of ending point (s)">
					<Definition position="node" valueType="real" default="0.0" option="false">
						<Condition type="isEqual" target="IPON_IN" value="1"/>
					</Definition>
				</Item>
				<Item name="AVRG_WELE" caption="          Averaged tidal level (m)">
					<Definition position="node" valueType="real" default="0.0" option="false">
						<Condition type="isEqual" target="IPON_IN" value="1"/>
					</Definition>
				</Item>
				<Item name="HYDOR_ELV_OUT" caption="          Time varying elevation (outflow)">
					<Definition conditionType="functional">
						<Parameter name="TIME_EL_OUT" valueType="real" caption="Time (s)"/>
						<Value name="ELV_OUT" valueType="real" caption="Water level (m)"/>
						<Condition type="isEqual" target="IPON_IN" value="2"/>
						<Action type="enable" />
					</Definition>
				</Item>
				<Item name="HYDOR_Q" caption="          Time varying discharge">
					<Definition conditionType="functional">
						<Parameter name="HYDRO_TIME" valueType="real" caption="Time (s)"/>
						<Value name="HYDRO_QQ" valueType="real" caption="Discharge (m3/s)"/>
						<Condition type="isEqual" target="IPON_IN" value="3"/>
						<Action type="enable" />
					</Definition>
				</Item>
				<Item name="H_Q_PARM_A" caption="          H - Q parameter [A] (Q=A*(H+B)**2)">
					<Definition position="node" valueType="real" default="0.0" option="false">
						<Condition type="isEqual" target="IPON_IN" value="4"/>
					</Definition>
				</Item>
				<Item name="H_Q_PARM_B" caption="          H - Q parameter [B] (Q=A*(H+B)**2)">
					<Definition position="node" valueType="real" default="0.0" option="false">
						<Condition type="isEqual" target="IPON_IN" value="4"/>
					</Definition>
				</Item>
				<Item name="HYDOR_Q_E" caption="          Time varying elevation and discharge">
					<Definition conditionType="functional">
						<Parameter name="TIME_Q_E" valueType="real" caption="Time (s)"/>
						<Value name="HYD_Q_E" valueType="real" caption="Water level (m)"/>
						<Value name="HYD_Q_Q" valueType="real" caption="Discharge (m3/s)"/>
						<Condition type="isEqual" target="IPON_IN" value="5"/>
						<Action type="enable" />
					</Definition>
				</Item>
				<Item name="HYDOR_ELV_IN" caption="          Time varying elevation (inflow)">
					<Definition conditionType="functional">
						<Parameter name="TIME_EL_IN" valueType="real" caption="Time (s)"/>
						<Value name="ELE_IN" valueType="real" caption="Water level (m)"/>
				<Condition type="isEqual" target="IPON_IN" value="6"/>
						<Action type="enable" />
					</Definition>
				</Item>
			</GroupBox>
			<GroupBox caption="Boundary condition of Movable bed computation">
				<GroupBox caption="Boundary condition of bed load">
					<Item name="IPON_BED" caption="     Type of boundary condition of bed load">
						<Definition valueType="integer" default="0" >
							<Enumeration value="0" caption="Equilibrium sediment discharge" />
							<Enumeration value="1" caption="Ratio of sediment discharge" />
							<Condition type="or">
					  			<Condition type="isEqual" target="IBCON_13" value="1"/>
			  					<Condition type="isEqual" target="IBCON_13" value="2"/>
							</Condition>
							<Action type="enable" />
						</Definition>
					</Item>
					<Item name="RATIO_ES" caption="          Ratio of sediment discharge (%)">
						<Definition position="node" valueType="real" default="100.0" option="false">
							<Condition type="and">
								<Condition type="or">
						  			<Condition type="isEqual" target="IBCON_13" value="1"/>
			  						<Condition type="isEqual" target="IBCON_13" value="2"/>
								</Condition>
								<Condition type="isEqual" target="IPON_BED" value="1"/>
							</Condition>
							<Action type="enable"/>
						</Definition>
					</Item>
				</GroupBox>
				<GroupBox caption="Boundary condition of suspended load">
					<Item name="IPON_SUS" caption="     Type of boundary condition of suspended load">
						 <Definition valueType="integer" default="0" >
							<Enumeration value="0" caption="Equilibrium sediment discharge" />
							<Enumeration value="1" caption="Fixed concentrarion of each diameter" />
							<Enumeration value="2" caption="Discharge - suspended load distribution" />
							<Enumeration value="3" caption="Periodic boundary condition" />
					  		<Condition type="isEqual" target="IBCON_13" value="2"/>
						</Definition>
					</Item>
					<Item name="VVS_ES" caption="          Fixed concentrarion of each diameter">
						<Definition conditionType="functional">
							<Parameter name="VVS_ID" valueType="real" caption="Diameter(mm)"/>
							<Value name="VSS_ES" valueType="real" caption="Concentraion"/>
							<Condition type="and">
								<Condition type="isEqual" target="IPON_SUS" value="1"/>
								<Condition type="isEqual" target="IBCON_13" value="2"/>
							</Condition>
							<Action type="enable" />
						</Definition>
					</Item>
					<Item name="QPS_SS" caption="          Discharge coefficient ( QP : QP * 10**IQ * Q**2 )">
						<Definition position="node" valueType="real" default="3.5" option="false">
							<Condition type="and">
								<Condition type="isEqual" target="IPON_SUS" value="2"/>
			  					<Condition type="isEqual" target="IBCON_13" value="2"/>
							</Condition>
						</Definition>
					</Item>
					<Item name="IQS_SS" caption="          Power coefficient ( IQ : QP * 10**IQ * Q**2 )">
						<Definition position="node" valueType="integer" default="-8" min="-10" option="false">
							<Condition type="and">
								<Condition type="isEqual" target="IPON_SUS" value="2"/>
			  					<Condition type="isEqual" target="IBCON_13" value="2"/>
							</Condition>
						</Definition>
					</Item>
					<Item name="LINN_ES" caption="          Appointted outflow boundary of periodic boundary condition">
						<Definition position="node" valueType="integer" default="1" option="false">
							<Condition type="and">
								<Condition type="isEqual" target="IPON_SUS" value="3"/>
			  					<Condition type="isEqual" target="IBCON_13" value="2"/>
							</Condition>
						</Definition>
					</Item>
					<Item name="RATIO_SS" caption="          Ratio of Appointted outflow boundary concentraion (%)">
						<Definition position="node" valueType="real" default="100.0" option="false">
							<Condition type="and">
								<Condition type="isEqual" target="IPON_SUS" value="3"/>
			  					<Condition type="isEqual" target="IBCON_13" value="2"/>
							</Condition>
						</Definition>
					</Item>
				</GroupBox>
			</GroupBox>
		</HBoxLayout>
	</BoundaryCondition>
</SolverDefinition>
