<?xml version="1.0" encoding="UTF-8"?>
<SolverDefinition 
    xmlns="www.iric.net/SolverDefinition/1.0"
    name="UTT" caption="GELATO(former UTT)" version="1.2.23122001" copyright="Yasuyuki SHIMIZU"
    release="2023.05.09"
    homepage="http://ws3-er.eng.hokudai.ac.jp/yasu/"
    executable="GELATO.exe"
    iterationtype="time"
    gridtype="structured2d,unstructured2d"
    >
	<CalculationCondition>

		<Tab name="basic_setting" caption="Basic Settings">
			
			<Item name="inputfile" caption="Flow information input file name">
				<Definition conditionType="constant" valueType="filename" default="Select CGNS file name" />
			</Item>
			<Item name="j_surface" caption="Velocity used for tracer tracking">
				<Definition conditionType="constant" valueType="integer" default="0">
					<Enumerations>
						<Enumeration value="0" caption="(0) depth averaged velocity"/>
						<Enumeration value="1" caption="(1) surface velocity"/>
						<Enumeration value="2" caption="(2) bottom velocity"/>
					</Enumerations>
				</Definition>
			</Item>
			<Item name="j_rep" caption="Periodic boundary condition for tracers">
				<Definition conditionType="constant" valueType="integer" default="0">
					<Enumerations>
						<Enumeration value="0" caption="Not Apply"/>
						<Enumeration value="1" caption="Apply"/>
					</Enumerations>
				</Definition>
			</Item>
			<Item name="cr_depth" caption="Minimum depth of tracer stopping(m)">
				<Definition conditionType="constant" valueType="real" default="0.003"/>
			</Item>
      		<Item name="j_treat" caption="Treatment when depth is shallower than minimum depth">
				<Definition conditionType="constant" valueType="integer" default="1">
					<Enumerations>
						<Enumeration value="1" caption="Stay still"/>
						<Enumeration value="2" caption="Disappear"/>
					</Enumerations>
				</Definition>
			</Item>
			<Item name="ddt" caption="Time interval for tracer tracking(sec)">
				<Definition conditionType="constant" valueType="real" default="0.001"/>
			</Item>
			<Item name="end_time" caption="Computation ending time(sec)">
				<Definition conditionType="constant" valueType="real" default="10"/>
			</Item>
			<Item name="moi" caption="Output frequency increase factor">
				<Definition conditionType="constant" valueType="integer" default="1" max="100"/>
			</Item>
			<Item name="j_c_draw" caption="Drawing center line">
				<Definition conditionType="constant" valueType="integer" default="0">
					<Enumerations>
						<Enumeration value="0" caption="Not Draw"/>
						<Enumeration value="1" caption="Draw"/>
					</Enumerations>
				</Definition>
			</Item>

		</Tab>

		<Tab name="normal_spread" caption="Primary Tracers Supplying Condition">
			
			<Item name="j_spread" caption="Tracer releasing">
				<Definition conditionType="constant" valueType="integer" default="0">
					<Enumerations>
						<Enumeration value="0" caption="No"/>
						<Enumeration value="1" caption="Yes"/>
					</Enumerations>
				</Definition>
			</Item>
			<Item name="maxTracerNumber" caption="Maximum number of total tracers">
				<Definition conditionType="constant" valueType="integer" default="100000" max="1000000">
					<Dependency>
						<Condition type="isEqual" target="j_spread" value="1"/>
					</Dependency>
				</Definition>
			</Item>
			<Item name="maxNumberInCell" caption="Maximum number of tracers in one cell">
				<Definition conditionType="constant" valueType="integer" default="10" max="100">
					<Dependency>
						<Condition type="isEqual" target="j_spread" value="1"/>
					</Dependency>
				</Definition>
			</Item>

			<GroupBox caption="Tracers Supply Range">

				<GroupBox caption="Xi(Streamwise) Direction">

					<Item name="xi_1" caption="xi_1">
						<Definition conditionType="constant" valueType="real" default="0.1" >
							<Condition type="isEqual" target="j_spread" value="1"/>
						</Definition>
					</Item>
					<Item name="xi_2" caption="xi_2">
						<Definition conditionType="constant" valueType="real" default="0.1">
							<Condition type="isEqual" target="j_spread" value="1"/>
						</Definition>
					</Item>
					<Item name="d_trace_xi" caption="d_xi">
						<Definition conditionType="constant" valueType="real" default="0.1">
							<Condition type="isEqual" target="j_spread" value="1"/>
						</Definition>
					</Item>

				</GroupBox>

				<GroupBox caption="Eta(Transverse) Direction">

					<Item name="eta_1" caption="eta_1">
						<Definition conditionType="constant" valueType="real" default="0.3" >
							<Condition type="isEqual" target="j_spread" value="1"/>
						</Definition>
					</Item>
					<Item name="eta_2" caption="eta_2">
						<Definition conditionType="constant" valueType="real" default="0.7">
							<Condition type="isEqual" target="j_spread" value="1"/>
						</Definition>
					</Item>
					<Item name="d_trace_eta" caption="d_eta">
						<Definition conditionType="constant" valueType="real" default="0.02">
							<Condition type="isEqual" target="j_spread" value="1"/>
						</Definition>
					</Item>

				</GroupBox>
			
				<Image src="tracer supply range p.png" title="tracer supply range1"/>

			</GroupBox>

		</Tab>

		<Tab name="Cloning_Primary" caption="- Cloning and Amalgamation for Primary Normal Tracers">

			<Item name="j_add" caption="Tracer cloning">
				<Definition conditionType="constant" valueType="integer" default="0">
					<Condition type="isEqual" target="j_spread" value="1"/>
					<Enumerations>
						<Enumeration value="0" caption="(0)No"/>
						<Enumeration value="1" caption="(1)Yes"/>
					</Enumerations>
				</Definition>
			</Item>

			<GroupBox caption="Cloning Method">

				<Item name="j_space" caption="Cloning condition or method">
					<Definition conditionType="constant" valueType="integer" default="0">
						<Dependency>
							<Condition type="and">
								<Condition type="isEqual" target="j_add" value="1"/>
								<Condition type="isEqual" target="j_spread" value="1"/>
							</Condition>
						</Dependency>
						<Enumerations>
							<Enumeration value="0" caption="All empty cells"/>
							<Enumeration value="1" caption="Cells with only one tracer"/>
							<Enumeration value="2" caption="Specified cells with one tracer"/>
						</Enumerations>
					</Definition>
				</Item>
				<Item name="max_generation" caption="Maximum cloning generations">
					<Definition conditionType="constant" valueType="integer" default="5" max="100">
						<Dependency>
							<Condition type="and">
								<Condition type="isEqual" target="j_add" value="1"/>
								<Condition type="isEqual" target="j_spread" value="1"/>
							</Condition>
						</Dependency>
					</Definition>
				</Item>
				<Item name="reduc_fac" caption="Cloning reduction factor for empty cells cloning">
					<Definition conditionType="constant" valueType="integer" default="1" max="1000">
						<Dependency>
							<Condition type="and">
								<Condition type="isEqual" target="j_add" value="1"/>
								<Condition type="isEqual" target="j_spread" value="1"/>
								<Condition type="isEqual" target="j_space" value="0"/>
							</Condition>
						</Dependency>
					</Definition>
				</Item>

			</GroupBox>

			<GroupBox caption="Amalgamation">

				<Item name="j_colligation" caption="Tracer amalgamation">
					<Definition conditionType="constant" valueType="integer" default="0">
						<Condition type="isEqual" target="j_spread" value="1"/>
						<Enumerations>
							<Enumeration value="0" caption="No"/>
							<Enumeration value="1" caption="Yes"/>
						</Enumerations>
					</Definition>
				</Item>
				<Item name="nColligation" caption="Number of tracer at which colligation is executed">
					<Definition conditionType="constant" valueType="integer" default="20">
						<Dependency>
							<Condition type="isEqual" target="j_colligation" value="1"/>
							<Condition type="isEqual" target="j_spread" value="1"/>
						</Dependency>
					</Definition>
				</Item>

			</GroupBox>

		</Tab>


		<Tab name="normal_spread2" caption="Secondary Tracers Supplying Condition">
			
			<Item name="j_spread2" caption="Tracer releasing">
				<Definition conditionType="constant" valueType="integer" default="0">
					<Enumerations>
						<Enumeration value="0" caption="No"/>
						<Enumeration value="1" caption="Yes"/>
					</Enumerations>
				</Definition>
			</Item>
			<Item name="maxTracerNumber2" caption="Maximum number of total tracers">
				<Definition conditionType="constant" valueType="integer" default="100000" max="1000000">
					<Dependency>
						<Condition type="isEqual" target="j_spread2" value="1"/>
					</Dependency>
				</Definition>
			</Item>
			<Item name="maxNumberInCell2" caption="Maximum number of tracers in one cell">
				<Definition conditionType="constant" valueType="integer" default="10" max="100">
					<Dependency>
						<Condition type="isEqual" target="j_spread2" value="1"/>
					</Dependency>
				</Definition>
			</Item>

			<GroupBox caption="Tracers Supply Range">

				<GroupBox caption="Xi(Streamwise) Direction">

					<Item name="xi_12" caption="xi_1">
						<Definition conditionType="constant" valueType="real" default="0.1" >
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Definition>
					</Item>
					<Item name="xi_22" caption="xi_2">
						<Definition conditionType="constant" valueType="real" default="0.1">
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Definition>
					</Item>
					<Item name="d_trace_xi2" caption="d_xi">
						<Definition conditionType="constant" valueType="real" default="0.1">
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Definition>
					</Item>

				</GroupBox>

				<GroupBox caption="Eta(Transverse) Direction">

					<Item name="eta_12" caption="eta_1">
						<Definition conditionType="constant" valueType="real" default="0.3" >
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Definition>
					</Item>
					<Item name="eta_22" caption="eta_2">
						<Definition conditionType="constant" valueType="real" default="0.7">
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Definition>
					</Item>
					<Item name="d_trace_eta2" caption="d_eta">
						<Definition conditionType="constant" valueType="real" default="0.02">
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Definition>
					</Item>

				</GroupBox>

				<Image src="tracer supply range s.png"/>

			</GroupBox>

    	</Tab>

		<Tab name="Cloning_Secondary" caption="- Cloning and Amalgamation for Secondary Normal Tracers">

			<Item name="j_add2" caption="Tracer cloning">
				<Definition conditionType="constant" valueType="integer" default="0">
					<Condition type="isEqual" target="j_spread2" value="1"/>
					<Enumerations>
						<Enumeration value="0" caption="(0)No"/>
						<Enumeration value="1" caption="(1)Yes"/>
					</Enumerations>
				</Definition>
			</Item>

			<GroupBox caption="Cloning Method">

				<Item name="j_space2" caption="Cloning condition or method">
					<Definition conditionType="constant" valueType="integer" default="0">
						<Dependency>
							<Condition type="and">
								<Condition type="isEqual" target="j_add2" value="1"/>
								<Condition type="isEqual" target="j_spread2" value="1"/>
							</Condition>
						</Dependency>
						<Enumerations>
							<Enumeration value="0" caption="All empty cells"/>
							<Enumeration value="1" caption="Cells with only one tracer"/>
							<Enumeration value="2" caption="Specified cells with one tracer"/>
						</Enumerations>
					</Definition>
				</Item>
				<Item name="max_generation2" caption="Maximum cloning generations">
					<Definition conditionType="constant" valueType="integer" default="5" max="100">
						<Dependency>
							<Condition type="and">
								<Condition type="isEqual" target="j_add2" value="1"/>
								<Condition type="isEqual" target="j_spread2" value="1"/>
							</Condition>
						</Dependency>
					</Definition>
				</Item>
				<Item name="reduc_fac2" caption="Cloning reduction factor for empty cells cloning">
					<Definition conditionType="constant" valueType="integer" default="1" max="1000">
						<Dependency>
							<Condition type="and">
								<Condition type="isEqual" target="j_add2" value="1"/>
								<Condition type="isEqual" target="j_spread2" value="1"/>
								<Condition type="isEqual" target="j_space2" value="0"/>
							</Condition>
						</Dependency>
					</Definition>
				</Item>

			</GroupBox>

			<GroupBox caption="Amalgamation">

				<Item name="j_colligation2" caption="Tracer amalgamation">
					<Definition conditionType="constant" valueType="integer" default="0">
						<Condition type="isEqual" target="j_spread2" value="1"/>
						<Enumerations>
							<Enumeration value="0" caption="No"/>
							<Enumeration value="1" caption="Yes"/>
						</Enumerations>
					</Definition>
				</Item>
				<Item name="nColligation2" caption="Number of tracer at which colligation is executed">
					<Definition conditionType="constant" valueType="integer" default="20">
						<Dependency>
							<Condition type="isEqual" target="j_colligation2" value="1"/>
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Dependency>
					</Definition>
				</Item>

			</GroupBox>

		</Tab>

    	<Tab name="time_settings" caption="Time Settings for Normal Tracers">

			<GroupBox caption="Time Parameters for Normal Tracers">

				<Item name="spread_start" caption="spread_start(sec)">
					<Definition conditionType="constant" valueType="real" default="0.0" >
						<Condition type="or">
							<Condition type="isEqual" target="j_spread" value="1"/>
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Condition>
					</Definition>
				</Item>
				<Item name="spread_end" caption="spread_end(sec)">
					<Definition conditionType="constant" valueType="real" default="10.0">
						<Condition type="or">
							<Condition type="isEqual" target="j_spread" value="1"/>
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Condition>  
					</Definition>
				</Item>
				<Item name="spread_interval" caption="spred_interval(sec)">
					<Definition conditionType="constant" valueType="real" default="0.1">
						<Condition type="or">
							<Condition type="isEqual" target="j_spread" value="1"/>
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Condition>  
					</Definition>
				</Item>

			</GroupBox>

		</Tab>

		<Tab name="special_spread" caption="Special Tracers for Trajectory Supplying Condition">
			
			<Item name="j_spread_tj" caption="Special tracer releasing">
				<Definition conditionType="constant" valueType="integer" default="0">
					<Enumerations>
						<Enumeration value="0" caption="No"/>
						<Enumeration value="1" caption="Yes"/>
					</Enumerations>
				</Definition>
			</Item>
			<Item name="maxTracerNumber_tj" caption="Maximum number of total Special Tracers">
				<Definition conditionType="constant" valueType="integer" default="100" max="100">
					<Dependency>
						<Condition type="isEqual" target="j_spread_tj" value="1"/>
					</Dependency>
				</Definition>
			</Item>
			<Item name="maxTrjTimes" caption="Maximum times of tracer trajectory saving">
				<Definition conditionType="constant" valueType="integer" default="10000" max="100000">
					<Dependency>
						<Condition type="isEqual" target="j_spread_tj" value="1"/>
					</Dependency>
				</Definition>
			</Item>
			<Item name="intervalTrj" caption="Interval of tracer trajectory saving">
				<Definition conditionType="constant" valueType="integer" default="10" min="1" max="1000">
					<Dependency>
						<Condition type="isEqual" target="j_spread_tj" value="1"/>
					</Dependency>
				</Definition>
			</Item>

			<GroupBox caption="Tracers Supply Range">

				<GroupBox caption="Xi(Streamwise) Direction">

					<Item name="xi_1_tj" caption="xi_1">
						<Definition conditionType="constant" valueType="real" default="0.1" >
								<Condition type="isEqual" target="j_spread_tj" value="1"/>
						</Definition>
					</Item>
					<Item name="xi_2_tj" caption="xi_2">
						<Definition conditionType="constant" valueType="real" default="0.1">
								<Condition type="isEqual" target="j_spread_tj" value="1"/>
						</Definition>
					</Item>
					<Item name="d_trace_xi_tj" caption="d_xi">
						<Definition conditionType="constant" valueType="real" default="0.1">
								<Condition type="isEqual" target="j_spread_tj" value="1"/>
						</Definition>
					</Item>

				</GroupBox>

				<GroupBox caption="Eta(Transverse) Direction">

					<Item name="eta_1_tj" caption="eta_1">
						<Definition conditionType="constant" valueType="real" default="0.3" >
								<Condition type="isEqual" target="j_spread_tj" value="1"/>
						</Definition>
					</Item>
					<Item name="eta_2_tj" caption="eta_2">
						<Definition conditionType="constant" valueType="real" default="0.7">
								<Condition type="isEqual" target="j_spread_tj" value="1"/>
						</Definition>
					</Item>
					<Item name="d_trace_eta_tj" caption="d_eta">
						<Definition conditionType="constant" valueType="real" default="0.02">
								<Condition type="isEqual" target="j_spread_tj" value="1"/>
						</Definition>
					</Item>

				</GroupBox>
				
				<Image src="tracer supply range t.png"/>

			</GroupBox>

			<GroupBox caption="Time of Special Tracers Injection(sec)">

				<Item name="spread_tj" caption="spread time(sec)">
					<Definition conditionType="constant" valueType="real" default="0.0" >
							<Condition type="isEqual" target="j_spread_tj" value="1"/>
					</Definition>
				</Item>

			</GroupBox>
				
		</Tab>

		<Tab name="Initial" caption="Background Flow Condition">

			<Item name="j_flow" caption="Back ground flow velocity condition">
				<Definition conditionType="constant" valueType="integer" default="0">
					<Enumerations>
						<Enumeration value="0" caption="(0) Use Simulated Value"/>
						<Enumeration value="1" caption="(1) Give Constant Value"/>
					</Enumerations>
				</Definition>
			</Item>
			<Item name="Ini_Vel_Flow" caption="Background flow velocity(m/s)">
				<Definition conditionType="constant" valueType="real" default="0.0" >
					<Dependency>
						<Condition type="and">
							<Condition type="isEqual" target="j_flow" value="1"/>
						</Condition>
						<Action type="enable"/>
					</Dependency>
				</Definition>
			</Item>
			<Item name="Ini_Depth" caption="Background depth(m)">
				<Definition conditionType="constant" valueType="real" default="0.1" >
					<Dependency>
						<Condition type="and">
							<Condition type="isEqual" target="j_flow" value="1"/>
						</Condition>
						<Action type="enable"/>
					</Dependency>
				</Definition>
			</Item>

		</Tab>

		<Tab name="Wind_Map" caption="Wind Map Like Plotting">

			<GroupBox caption="Wind Map Setting">

				<Item name="j_winMap" caption="Drawing wind map">
					<Definition conditionType="constant" valueType="integer" default="0">
						<Enumerations>
							<Enumeration value="0" caption="(0) No"/>
							<Enumeration value="1" caption="(1) Yes"/>
						</Enumerations>
					</Definition>
				</Item>
				<Item name="n_wm" caption="Max numbers of wind map lines">
					<Definition conditionType="constant" valueType="integer" default="100" max="1000000">
						<Dependency>
							<Condition type="isEqual" target="j_winMap" value="1"/>
						</Dependency>
					</Definition>
				</Item>

				<!--      
				<Item name="maxLenWm" caption="Max life cycle step length of Windmap lines">
						<Definition conditionType="constant" valueType="integer" default="100" max="1000">
						<Dependency>
							<Condition type="isEqual" target="j_winMap" value="1"/>
						</Dependency>
						</Definition>
				</Item>
				-->

				<Item name="wmCtime" caption="Lifespan of each wind map lines(sec)">
					<Definition conditionType="constant" valueType="real" default="10">
						<Dependency>
							<Condition type="isEqual" target="j_winMap" value="1"/>
						</Dependency>
					</Definition>
				</Item>

				<!--      
				<Item name="stepWm" caption="Step interval of WinMap lines memory">
						<Definition conditionType="constant" valueType="integer" default="10" max="1000">
						<Dependency>
							<Condition type="isEqual" target="j_winMap" value="1"/>
						</Dependency>
						</Definition>
				</Item>
				-->

				<Item name="wmStime" caption="Time step for wind map memory(sec)">
					<Definition conditionType="constant" valueType="real" default="1" >
						<Dependency>
							<Condition type="isEqual" target="j_winMap" value="1"/>
						</Dependency>
					</Definition>
				</Item>

			</GroupBox>

		</Tab>


		<Tab name="fish_motion(1)" caption="Simulation of Fish Motion(1)">

			<Item name="j_fish" caption="Fish simulation">
				<Definition conditionType="constant" valueType="integer" default="0">
					<Enumerations>
						<Enumeration value="0" caption="No"/>
		 	   			<Enumeration value="1" caption="Yes"/>
					</Enumerations>
				</Definition>
			</Item>
			<Item name="j_rep_fish" caption="Periodic boundary condition for fish simulation">
				<Definition conditionType="constant" valueType="integer" default="0">
					<Dependency>
						<Condition type="isEqual" target="j_fish" value="1"/>
					</Dependency>
					<Enumerations>
						<Enumeration value="0" caption="No"/>
		 	   			<Enumeration value="1" caption="Yes"/>
					</Enumerations>
				</Definition>
			</Item>
			<Item name="vel_fish" caption="Fish cruising speed(m/s)">
				<Definition conditionType="constant" valueType="real" default="0.1" >
					<Condition type="isEqual" target="j_fish" value="1"/>
				</Definition>
			</Item>

			<!--
			<Item name="vf_critical" caption="Minimum Velocity Fish Can Feel(m/s)">
				<Definition conditionType="constant" valueType="real" default="0.01" >
						<Condition type="isEqual" target="j_fish" value="1"/>
				</Definition>
			</Item>
			-->

			<Item name="alphaCruise" caption="Cruising time to fish length ratio(s/m)">
				<Definition conditionType="constant" valueType="real" default="10." >
					<Condition type="isEqual" target="j_fish" value="1"/>
				</Definition>
			</Item>
			<Item name="tRatio" caption="Rush time to cruising cycle time ratio">
				<Definition conditionType="constant" valueType="real" default="0.1" >
					<Condition type="isEqual" target="j_fish" value="1"/>
				</Definition>
			</Item>
			<Item name="vRatio" caption="Rush speed to cruising speed ratio">
				<Definition conditionType="constant" valueType="real" default="5" >
					<Condition type="isEqual" target="j_fish" value="1"/>
				</Definition>
			</Item>
			<Item name="j_vel_fish" caption="Fish velocity variation">
				<Definition conditionType="constant" valueType="integer" default="1">
					<Condition type="isEqual" target="j_fish" value="1"/>
					<Enumerations>
						<Enumeration value="0" caption="Constant cruising velocity"/>
		 	   			<Enumeration value="1" caption="Cruising velocity depends on body length"/>
					</Enumerations>
				</Definition>
			</Item>
			<Item name="fishLength" caption="Average fish body length(m)">
				<Definition conditionType="constant" valueType="real" default="0.3" >
					<Condition type="isEqual" target="j_fish" value="1"/>
				</Definition>
			</Item>
			<Item name="j_fishLength" caption="Fish length variation">
				<Definition conditionType="constant" valueType="integer" default="0">
					<Condition type="isEqual" target="j_fish" value="1"/>
					<Enumerations>
						<Enumeration value="0" caption="Constant length"/>
		 	   			<Enumeration value="1" caption="Variation length"/>
					</Enumerations>
				</Definition>
			</Item>
			<Item name="j_valRange" caption="Variation range">
