<?xml version="1.0" encoding="UTF-8"?>
<SolverDefinition 
    xmlns="www.iric.net/SolverDefinition/1.0"
    name="UTT" caption="GELATO(former UTT)" version="1.3.24041501" copyright="Yasuyuki SHIMIZU"
    release="2024.04.15"
    homepage="https://i-ric.org/yasu/"
    executable="GELATO.exe"
    iterationtype="time"
    gridtype="structured2d"
    >
	<CalculationCondition>

		<Tab name="basic_setting" caption="Basic Settings">
			
			<Item name="inputfile" caption="Flow information input file name">
				<Definition valueType="filename" default="Select CGNS file name" />
			</Item>
			<Item name="j_surface" caption="Velocity used for tracer tracking">
				<Definition valueType="integer" default="0">
					<Enumerations>
						<Enumeration value="0" caption="(0) depth averaged velocity"/>
						<Enumeration value="1" caption="(1) surface velocity"/>
						<Enumeration value="2" caption="(2) bottom velocity"/>
					</Enumerations>
				</Definition>
			</Item>
			<Item name="j_rep" caption="Periodic boundary condition for Tracers">
				<Definition valueType="integer" default="0" checkBox="true"/>
			</Item>
			<Item name="cr_depth" caption="Critical depth for tracer motion(m)">
				<Definition valueType="real" default="0.003"/>
			</Item>
      		<Item name="j_treat" caption="Handling of tracer stoppage due to minimum water depth &#xa; or critical friction velocity">
				<Definition valueType="integer" default="1">
					<Enumerations>
						<Enumeration value="1" caption="Stop movement"/>
						<Enumeration value="2" caption="Disappear"/>
					</Enumerations>
				</Definition>
			</Item>
			<Item name="ddt" caption="Delta T for Particle Tracking">
				<Definition valueType="real" default="0.001"/>
			</Item>
			<Item name="end_time" caption="Computation Finishing Time(sec)">
				<Definition valueType="real" default="10"/>
			</Item>
			<Item name="moi" caption="Output frequency increase factor">
				<Definition valueType="integer" default="1" max="100"/>
			</Item>
			<Item name="j_c_draw" caption="Drawing Center Line">
				<Definition valueType="integer" default="0" checkBox="true"/>
			</Item>

		</Tab>

		<Tab name="normal_spread" caption="Primary Tracers Supplying Condition">
			
			<Item name="j_spread" caption="Tracer Releasing">
				<Definition valueType="integer" default="0" checkBox="true"/>
			</Item>
			<Item name="maxTracerNumber" caption="Maximum number of total tracer particles">
				<Definition valueType="integer" default="100000" max="1000000">
					<Dependency>
						<Condition type="isEqual" target="j_spread" value="1"/>
					</Dependency>
				</Definition>
			</Item>
			<Item name="maxNumberInCell" caption="Maximum number of tracer particles in one cell">
				<Definition valueType="integer" default="10" max="100">
					<Dependency>
						<Condition type="isEqual" target="j_spread" value="1"/>
					</Dependency>
				</Definition>
			</Item>
			<Item name="cr_usta" caption="Critical shear velocity below which the tracer stops(m/s)">
				<Definition valueType="real" default="0.0" >
					<Condition type="isEqual" target="j_spread" value="1"/>
				</Definition>
			</Item>
			
			<GroupBox caption="Tracers Supply Range">

				<GroupBox caption="Xi(Streamwise) Direction">

					<Item name="xi_1" caption="xi_1">
						<Definition valueType="real" default="0.1" >
							<Condition type="isEqual" target="j_spread" value="1"/>
						</Definition>
					</Item>
					<Item name="xi_2" caption="xi_2">
						<Definition valueType="real" default="0.1">
							<Condition type="isEqual" target="j_spread" value="1"/>
						</Definition>
					</Item>
					<Item name="d_trace_xi" caption="d_trace_xi">
						<Definition valueType="real" default="0.1">
							<Condition type="isEqual" target="j_spread" value="1"/>
						</Definition>
					</Item>

				</GroupBox>

				<GroupBox caption="Eta(Transverse) Direction">

					<Item name="eta_1" caption="eta_1">
						<Definition valueType="real" default="0.3" >
							<Condition type="isEqual" target="j_spread" value="1"/>
						</Definition>
					</Item>
					<Item name="eta_2" caption="eta_2">
						<Definition valueType="real" default="0.7">
							<Condition type="isEqual" target="j_spread" value="1"/>
						</Definition>
					</Item>
					<Item name="d_trace_eta" caption="d_trace_eta">
						<Definition valueType="real" default="0.02">
							<Condition type="isEqual" target="j_spread" value="1"/>
						</Definition>
					</Item>

				</GroupBox>

				<Image src="tracer supply range.png"/>

			</GroupBox>

		</Tab>

		<Tab name="Cloning_Primary" caption="&#x20;- Cloning and Amalgamation for Primary Normal Tracers">

			<Item name="j_add" caption="Tracer Cloning for Primary Normal Tracers">
				<Definition valueType="integer" default="0" checkBox="true">
					<Condition type="isEqual" target="j_spread" value="1"/>
				</Definition>
			</Item>

			<GroupBox caption="Cloning Method">

				<Item name="j_space" caption="Cloning condition or method">
					<Definition valueType="integer" default="0">
						<Dependency>
							<Condition type="and">
								<Condition type="isEqual" target="j_add" value="1"/>
								<Condition type="isEqual" target="j_spread" value="1"/>
							</Condition>
						</Dependency>
						<Enumerations>
							<Enumeration value="0" caption="All empty cells"/>
							<Enumeration value="1" caption="Cells with only one particle"/>
							<Enumeration value="2" caption="Specified cells with one particle"/>
						</Enumerations>
					</Definition>
				</Item>
				<Item name="max_generation" caption="Maximum Cloning Generations">
					<Definition valueType="integer" default="5" max="100">
						<Dependency>
							<Condition type="and">
								<Condition type="isEqual" target="j_add" value="1"/>
								<Condition type="isEqual" target="j_spread" value="1"/>
							</Condition>
						</Dependency>
					</Definition>
				</Item>
				<Item name="reduc_fac" caption="Cloning reduction factor for empty cells cloning">
					<Definition valueType="integer" default="1" max="1000">
						<Dependency>
							<Condition type="and">
								<Condition type="isEqual" target="j_add" value="1"/>
								<Condition type="isEqual" target="j_spread" value="1"/>
								<Condition type="isEqual" target="j_space" value="0"/>
							</Condition>
						</Dependency>
					</Definition>
				</Item>

			</GroupBox>

			<GroupBox caption="Amalgamation">

				<Item name="j_colligation" caption="Amalgamation for Primary Normal Tracers">
					<Definition valueType="integer" default="0" checkBox="true">
						<Condition type="isEqual" target="j_spread" value="1"/>
					</Definition>
				</Item>
				<Item name="nColligation" caption="Number of particle at which colligation is executed">
					<Definition valueType="integer" default="20">
						<Dependency>
							<Condition type="isEqual" target="j_colligation" value="1"/>
							<Condition type="isEqual" target="j_spread" value="1"/>
						</Dependency>
					</Definition>
				</Item>

			</GroupBox>

		</Tab>


		<Tab name="normal_spread2" caption="Secondary Tracers Supplying Condition">
			
			<Item name="j_spread2" caption="Tracer Releasing">
				<Definition valueType="integer" default="0" checkBox="true"/>
			</Item>
				<Item name="maxTracerNumber2" caption="Maximum number of total tracer particles">
				<Definition valueType="integer" default="100000" max="1000000">
					<Dependency>
						<Condition type="isEqual" target="j_spread2" value="1"/>
					</Dependency>
				</Definition>
			</Item>
				<Item name="maxNumberInCell2" caption="Maximum number of tracer particles in one cell">
				<Definition valueType="integer" default="10" max="100">
					<Dependency>
						<Condition type="isEqual" target="j_spread2" value="1"/>
					</Dependency>
				</Definition>
			</Item>
			<Item name="cr_usta2" caption="Critical shear velocity below which the tracer stops(m/s)">
				<Definition valueType="real" default="0.0" >
					<Condition type="isEqual" target="j_spread2" value="1"/>
				</Definition>
			</Item>
			<GroupBox caption="Tracers Supply Range">

				<GroupBox caption="Xi(Streamwise) Direction">

					<Item name="xi_12" caption="xi_1">
						<Definition valueType="real" default="0.1" >
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Definition>
					</Item>
					<Item name="xi_22" caption="xi_2">
						<Definition valueType="real" default="0.1">
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Definition>
					</Item>
					<Item name="d_trace_xi2" caption="d_trace_xi">
						<Definition valueType="real" default="0.1">
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Definition>
					</Item>

				</GroupBox>

				<GroupBox caption="Eta(Transverse) Direction">

					<Item name="eta_12" caption="eta_1">
						<Definition valueType="real" default="0.3" >
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Definition>
					</Item>
					<Item name="eta_22" caption="eta_2">
						<Definition valueType="real" default="0.7">
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Definition>
					</Item>
					<Item name="d_trace_eta2" caption="d_trace_eta">
						<Definition valueType="real" default="0.02">
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Definition>
					</Item>

				</GroupBox>

				<Image src="tracer supply range.png"/>

			</GroupBox>

    	</Tab>

		<Tab name="Cloning_Secondary" caption="&#x20;- Cloning and Amalgamation for Secondary Normal Tracers">

			<Item name="j_add2" caption="Tracer Cloning for Secondary Normal Tracers">
				<Definition valueType="integer" default="0" checkBox="true">
					<Condition type="isEqual" target="j_spread2" value="1"/>
				</Definition>
			</Item>

			<GroupBox caption="Cloning Method">

				<Item name="j_space2" caption="Cloning condition or method">
					<Definition valueType="integer" default="0">
						<Dependency>
							<Condition type="and">
								<Condition type="isEqual" target="j_add2" value="1"/>
								<Condition type="isEqual" target="j_spread2" value="1"/>
							</Condition>
						</Dependency>
						<Enumerations>
							<Enumeration value="0" caption="All empty cells"/>
							<Enumeration value="1" caption="Cells with only one particle"/>
							<Enumeration value="2" caption="Specified cells with one particle"/>
						</Enumerations>
					</Definition>
				</Item>
				<Item name="max_generation2" caption="Maximum Cloning Generations">
					<Definition valueType="integer" default="5" max="100">
						<Dependency>
							<Condition type="and">
								<Condition type="isEqual" target="j_add2" value="1"/>
								<Condition type="isEqual" target="j_spread2" value="1"/>
							</Condition>
						</Dependency>
					</Definition>
				</Item>
				<Item name="reduc_fac2" caption="Cloning reduction factor for empty cells cloning">
					<Definition valueType="integer" default="1" max="1000">
						<Dependency>
							<Condition type="and">
								<Condition type="isEqual" target="j_add2" value="1"/>
								<Condition type="isEqual" target="j_spread2" value="1"/>
								<Condition type="isEqual" target="j_space2" value="0"/>
							</Condition>
						</Dependency>
					</Definition>
				</Item>

			</GroupBox>

			<GroupBox caption="Amalgamation">

				<Item name="j_colligation2" caption="Amalgamation for Secondary Normal Tracers">
					<Definition valueType="integer" default="0" checkBox="true">
						<Condition type="isEqual" target="j_spread2" value="1"/>
					</Definition>
				</Item>
				<Item name="nColligation2" caption="Number of particle at which colligation is executed">
					<Definition valueType="integer" default="20">
						<Dependency>
							<Condition type="isEqual" target="j_colligation2" value="1"/>
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Dependency>
					</Definition>
				</Item>

			</GroupBox>

		</Tab>

    	<Tab name="time_settings" caption="Time Settings for Normal Tracers">
				<GroupBox caption="Time Parameters for Normal Tracers(Seconds)">
					<Item name="spread_start" caption="spread_start">
					<Definition valueType="real" default="0.0" >
						<Condition type="or">
							<Condition type="isEqual" target="j_spread" value="1"/>
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Condition>
					</Definition>
				</Item>
					<Item name="spread_end" caption="spread_end">
					<Definition valueType="real" default="10.0">
						<Condition type="or">
							<Condition type="isEqual" target="j_spread" value="1"/>
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Condition>  
					</Definition>
				</Item>
					<Item name="spread_interval" caption="spred_interval">
					<Definition valueType="real" default="0.1">
						<Condition type="or">
							<Condition type="isEqual" target="j_spread" value="1"/>
							<Condition type="isEqual" target="j_spread2" value="1"/>
						</Condition>  
					</Definition>
				</Item>

			</GroupBox>

		</Tab>

		<Tab name="special_spread" caption="Special Tracers for Trajectory Supplying Condition">
			
			<Item name="j_spread_tj" caption="Release Tracers for Trajectory">
				<Definition valueType="integer" default="0" checkBox="true"/>
			</Item>
			<Item name="maxTracerNumber_tj" caption="Maximum number of total tracer particles for Trajectory">
				<Definition valueType="integer" default="100" max="100">
					<Dependency>
						<Condition type="isEqual" target="j_spread_tj" value="1"/>
					</Dependency>
				</Definition>
			</Item>
			<Item name="maxTrjTimes" caption="Maximum Times of Tracer Trajectory Saving">
				<Definition valueType="integer" default="10000" max="100000">
					<Dependency>
						<Condition type="isEqual" target="j_spread_tj" value="1"/>
					</Dependency>
				</Definition>
			</Item>
			<Item name="intervalTrj" caption="Interval of Tracer Trajectory Saving">
				<Definition valueType="integer" default="10" min="1" max="1000">
					<Dependency>
						<Condition type="isEqual" target="j_spread_tj" value="1"/>
					</Dependency>
				</Definition>
			</Item>

			<GroupBox caption="Tracers Supply Range">

				<GroupBox caption="Xi(Streamwise) Direction">

					<Item name="xi_1_tj" caption="xi_1">
						<Definition valueType="real" default="0.1" >
								<Condition type="isEqual" target="j_spread_tj" value="1"/>
						</Definition>
					</Item>
					<Item name="xi_2_tj" caption="xi_2">
						<Definition valueType="real" default="0.1">
								<Condition type="isEqual" target="j_spread_tj" value="1"/>
						</Definition>
					</Item>
					<Item name="d_trace_xi_tj" caption="d_xi">
						<Definition valueType="real" default="0.1">
								<Condition type="isEqual" target="j_spread_tj" value="1"/>
						</Definition>
					</Item>

				</GroupBox>

				<GroupBox caption="Eta(Transverse) Direction">

					<Item name="eta_1_tj" caption="eta_1">
						<Definition valueType="real" default="0.3" >
								<Condition type="isEqual" target="j_spread_tj" value="1"/>
						</Definition>
					</Item>
					<Item name="eta_2_tj" caption="eta_2">
						<Definition valueType="real" default="0.7">
								<Condition type="isEqual" target="j_spread_tj" value="1"/>
						</Definition>
					</Item>
					<Item name="d_trace_eta_tj" caption="d_eta">
						<Definition valueType="real" default="0.02">
								<Condition type="isEqual" target="j_spread_tj" value="1"/>
						</Definition>
					</Item>

				</GroupBox>
				
				<Image src="tracer supply range.png"/>

			</GroupBox>

			<GroupBox caption="Time of Special Tracers Injection(Seconds)">

				<Item name="spread_tj" caption="spread time(sec)">
					<Definition valueType="real" default="0.0" >
							<Condition type="isEqual" target="j_spread_tj" value="1"/>
					</Definition>
				</Item>

			</GroupBox>
				
		</Tab>

		<Tab name="Initial" caption="Background Flow Condition">

			<Item name="j_flow" caption="Back Ground Flow Velocity Condition">
				<Definition valueType="integer" default="0">
					<Enumerations>
						<Enumeration value="0" caption="(0) Use Simulated Value"/>
						<Enumeration value="1" caption="(1) Give Constant Value"/>
					</Enumerations>
				</Definition>
			</Item>
			<Item name="Ini_Vel_Flow" caption="Background Flow Velocity(m/s)">
				<Definition valueType="real" default="0.0" >
					<Dependency>
						<Condition type="and">
							<Condition type="isEqual" target="j_flow" value="1"/>
						</Condition>
						<Action type="enable"/>
					</Dependency>
				</Definition>
			</Item>
			<Item name="Ini_Depth" caption="Background Depth(m)">
				<Definition valueType="real" default="0.1" >
					<Dependency>
						<Condition type="and">
							<Condition type="isEqual" target="j_flow" value="1"/>
						</Condition>
						<Action type="enable"/>
					</Dependency>
				</Definition>
			</Item>

		</Tab>

		<Tab name="Wind_Map" caption="Wind Map Like Plotting">

			<GroupBox caption="Wind Map Setting (Only for Primary Normal Tracers)">

				<Item name="j_winMap" caption="Drawing Windmap">
					<Definition valueType="integer" default="0" checkBox="true"/>
				</Item>
				<Item name="n_wm" caption="Max numbers of windmap lines">
					<Definition valueType="integer" default="100" max="1000000">
						<Dependency>
							<Condition type="isEqual" target="j_winMap" value="1"/>
						</Dependency>
					</Definition>
				</Item>

				<Item name="wmCtime" caption="Lifespan of each Windmap lines(sec)">
					<Definition valueType="real" default="10">
						<Dependency>
							<Condition type="isEqual" target="j_winMap" value="1"/>
						</Dependency>
					</Definition>
				</Item>

				<Item name="wmStime" caption="Time step for WindMap memory(sec)">
					<Definition valueType="real" default="1" >
						<Dependency>
							<Condition type="isEqual" target="j_winMap" value="1"/>
						</Dependency>
					</Definition>
				</Item>

			</GroupBox>

		</Tab>


		<Tab name="fish_motion(1)" caption="Simulation of Fish Motion(1)">

			<Item name="j_fish" caption="Fish Simulation">
				<Definition valueType="integer" default="0" checkBox="true"/>
			</Item>
			<Item name="j_rep_fish" caption="Periodic Boundary Condition for Fish Simulation">
				<Definition valueType="integer" default="0" checkBox="true">
					<Dependency>
						<Condition type="isEqual" target="j_fish" value="1"/>
					</Dependency>
				</Definition>
			</Item>
			<Item name="vel_fish" caption="Fish Cruising Speed(m/s)">
				<Definition valueType="real" default="0.1" >
					<Condition type="isEqual" target="j_fish" value="1"/>
				</Definition>
			</Item>

			<Item name="alphaCruise" caption="Cruising time to fish length ratio(s/m)">
				<Definition valueType="real" default="10." >
					<Condition type="isEqual" target="j_fish" value="1"/>
				</Definition>
			</Item>
			<Item name="tRatio" caption="Rush time to cruising cycle time ratio">
				<Definition valueType="real" default="0.1" >
					<Condition type="isEqual" target="j_fish" value="1"/>
				</Definition>
			</Item>
			<Item name="vRatio" caption="Rush speed to cruising speed ratio">
				<Definition valueType="real" default="5" >
					<Condition type="isEqual" target="j_fish" value="1"/>
				</Definition>
			</Item>
			<Item name="j_vel_fish" caption="Fish velocity variation">
				<Definition valueType="integer" default="1">
					<Condition type="isEqual" target="j_fish" value="1"/>
					<Enumerations>
						<Enumeration value="0" caption="Constant cruising velocity"/>
		 	   			<Enumeration value="1" caption="Cruising velocity depends on body length"/>
					</Enumerations>
				</Definition>
			</Item>
			<Item name="fishLength" caption="Average Fish Body Length(m)">
				<Definition valueType="real" default="0.3" >
					<Condition type="isEqual" target="j_fish" value="1"/>
				</Definition>
			</Item>
			<Item name="j_fishLength" caption="Fish length variation">
				<Definition valueType="integer" default="0">
					<Condition type="isEqual" target="j_fish" value="1"/>
					<Enumerations>
						<Enumeration value="0" caption="Constant length"/>
		 	   			<Enumeration value="1" caption="Variation length"/>
					</Enumerations>
				</Definition>
			</Item>
			<Item name="j_valRange" caption="Variation Range">
				<Definition valueType="integer" default="1">
					<Dependency>
						<Condition type="and">
							<Condition type="isEqual" target="j_fish" value="1"/>
							<Condition type="isEqual" target="j_fishLength" value="1"/>
            			</Condition>
        			</Dependency>    
					<Enumerations>
						<Enumeration value="1" caption="0.5--1.5"/>
						<Enumeration value="2" caption="0.7--1.3"/>
						<Enumeration value="3" caption="0.9--1.1"/>
					</Enumerations>
				</Definition>
			</Item>
			<Item name="icol_t" caption="Number of Fish Sizes(Colors)">
				<Definition valueType="integer" default="10" >
					<Condition type="isEqual" target="j_fish" value="1"/>
				</Definition>
			</Item>

			<GroupBox caption="Fish Jumping parameters">

				<Item name="j_fishJump" caption="Fish Jumping">
					<Definition valueType="integer" default="0" checkBox="true">
						<Condition type="isEqual" target="j_fish" value="1"/>
					</Definition>
				</Item>
				<Item name="try_height" caption="Bed elevation drop height where fish consider jumping(m)">
					<Definition valueType="real" default="0.15" >
						<Dependency>
							<Condition type="and">
								<Condition type="isEqual" target="j_fish" value="1"/>
								<Condition type="isEqual" target="j_fishJump" value="1"/>
							</Condition>
						</Dependency>    
					</Definition>
				</Item>
				<Item name="JumpHeight" caption="Possible jumping height(m)">
					<Definition valueType="real" default="0.5" >
						<Dependency>
							<Condition type="and">
								<Condition type="isEqual" target="j_fish" value="1"/>
								<Condition type="isEqual" target="j_fishJump" value="1"/>
							</Condition>
						</Dependency>    
					</Definition>
				</Item>
				<Item name="JumpDistance" caption="Possible jumping distance(m)">
					<Definition valueType="real" default="0.5" >
						<Dependency>
							<Condition type="and">
								<Condition type="isEqual" target="j_fish" value="1"/>
								<Condition type="isEqual" target="j_fishJump" value="1"/>
							</Condition>
						</Dependency>    
					</Definition>
				</Item>
				<Item name="TimeCchange" caption="Color change time during jumping motion(sec)">
					<Definition valueType="real" default="1" >
						<Dependency>
							<Condition type="and">
								<Condition type="isEqual" target="j_fish" value="1"/>
								<Condition type="isEqual" target="j_fishJump" value="1"/>
							</Condition>
						</Dependency>    
					</Definition>
				</Item>

			</GroupBox>

    </Tab>

		<Tab name="fish_motion(2)" caption="Simulation of Fish Motion(2)">
      
			<Item name="j_fishLocation" caption="How to Set Initial Location">
				<Definition valueType="integer" default="1">
					<Condition type="isEqual" target="j_fish" value="1"/>
					<Enumerations>
						<Enumeration value="1" caption="Nondimensional Coordinate"/>
						<Enumeration value="2" caption="Give total numbers and random placement"/>
					</Enumerations>
				</Definition>
