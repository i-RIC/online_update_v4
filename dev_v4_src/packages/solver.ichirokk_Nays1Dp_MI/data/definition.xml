    <Item name="dl_mix" caption="Mixing Layer Thickness[m]">
     <Definition conditionType="constant" valueType="real" option="false" default="0.1" >
      <Dependency>
			 <Condition type="and">
         <Condition type="isEqual" target="m_bed" value="1"/>
