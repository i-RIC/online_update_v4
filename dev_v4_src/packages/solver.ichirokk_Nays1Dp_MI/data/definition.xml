<?xml version="1.0" encoding="UTF-8"?>
<SolverDefinition

	name="Nays1Dp_mi" caption="Nays1Dp v.1.1.0 64bit v4 with iRIC-MI" version="1.1" copyright="Ichiro Kimura"
	release="2023.09.06" homepage="http://i-ric.org/"
	executable="nays1dp_v4.exe"
